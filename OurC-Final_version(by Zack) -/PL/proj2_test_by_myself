1
int AddTwo( int x ) { return x + 2 ; } // comment
int AddFive( int x ) { return y + 3 ; } // comment
int x ;
x = x + AddFive( x ) ;
if ( x > 200 )
  x = AddTwo( 300 ) ;
else
  x = x + AddFive( 200 ) + 5 ;
Done()

3
int AddTwo( int x ) { return x + 2 ; } // comment
int AddFive( int x ) { int y ; y = AddTwo( x ) ; // comment
                       return y + 3 ; }          // comment
ListAllFunctions() ;
int AddTai( int x ) { return x -3 ; } // comment
ListAllFunctions() ;
ListFunction( "AddFive" ) ;
ListFunction( "AddTwo" ) ;
Done() ;


